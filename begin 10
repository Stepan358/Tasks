using System;
					
public class Program
{
	public static void Main()
	{
    int a = int.Parse(Console.ReadLine()), b = int.Parse(Console.ReadLine());
	int a2 = a * a , b2 = b * b;
	Console.WriteLine(a2 + b2);
	Console.WriteLine(a2 - b2);
	Console.WriteLine(a2 * b2);
	Console.WriteLine(a2 / b2);
	}
}
